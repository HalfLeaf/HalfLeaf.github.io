ct{$index}\n    &, .theme-light &\n      background-color lighten($color, 90%)\n\n      &:hover\n        background-color lighten($color, 75%)\n\n    .theme-dark &\n      background-color darken($color, 75%)\n\n      &:hover\n        background-color darken($color, 60%)\n</style>\n"]}]}